    "./taiwan/tel": {
      "types": "./dist/taiwan/tel.d.ts",
      "import": "./dist/taiwan/tel.js",
      "require": "./dist/taiwan/tel.cjs"
    }
  },
  "scripts": {
    "build": "tsup",
    "test": "vitest",
    "prepublishOnly": "npm test -- --run && npm run build"
  },
  "dependencies": {
    "dayjs": "^1.11.19"
  },
  "peerDependencies": {
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@types/node": "^24.10.12",
    "eslint": "^9.39.2",
    "jiti": "^2.6.1",
    "prettier": "^3.8.1",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.54.0",
    "vitest": "^4.0.18"
  }
