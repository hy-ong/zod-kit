      "require": "./dist/common/credit-card.cjs"
    },
    "./common/date": {
      "types": "./dist/common/date.d.ts",
      "import": "./dist/common/date.js",
      "require": "./dist/common/date.cjs"
    },
    "./common/datetime": {
