      "import": "./dist/common/ip.js",
      "require": "./dist/common/ip.cjs"
    },
    "./common/number": {
      "types": "./dist/common/number.d.ts",
      "import": "./dist/common/number.js",
      "require": "./dist/common/number.cjs"
    },
    "./common/password": {
      "types": "./dist/common/password.d.ts",
