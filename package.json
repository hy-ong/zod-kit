      "require": "./dist/taiwan/license-plate.cjs"
    },
    "./taiwan/mobile": {
      "types": "./dist/taiwan/mobile.d.ts",
      "import": "./dist/taiwan/mobile.js",
      "require": "./dist/taiwan/mobile.cjs"
    },
    "./taiwan/national-id": {
      "types": "./dist/taiwan/national-id.d.ts",
