{
  "name": "@hy_ong/zod-kit",
  "version": "0.2.1",
  "description": "A comprehensive TypeScript library providing pre-built Zod validation schemas with full internationalization support for common data types and Taiwan-specific formats",
  "keywords": [
    "zod",
    "validation",
    "schema",
    "typescript",
    "taiwan",
    "i18n",
    "form-validation",
    "email",
    "password",
    "phone",
    "postal-code",
    "national-id",
    "business-id"
  ],
  "homepage": "https://github.com/hy-ong/zod-kit#readme",
  "bugs": {
    "url": "https://github.com/hy-ong/zod-kit/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hy-ong/zod-kit.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "license": "MIT",
  "author": "Ong Hoe Yuan",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "default": "./dist/index.js"
    },
    "./common/boolean": {
      "types": "./dist/common/boolean.d.ts",
